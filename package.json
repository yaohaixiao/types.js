{
  "name": "@yaohaixiao/types.js",
  "version": "2.7.0",
  "description": "types.js - A library of tools focused on detecting JavaScript data types.",
  "main": "index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "start": "gulp start",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "build:lib": "npm run prettier:write && rollup -c build/rollup.config.lib.js --bundleConfigAsCjs && npm run build:.d.ts",
    "build:.d.ts": "tsc",
    "build:api": "npm run prettier:write && rollup -c build/rollup.config.api.js --bundleConfigAsCjs",
    "build:docs": "gulp build",
    "build:only": "npm run build:lib && npm run build:docs",
    "build": "npm run test:unit && npm run build:lib && npm run build:docs",
    "lint": "eslint",
    "lint:fix": "npm run lint --cache --fix",
    "prettier:check": "npx prettier --check \"./*.js\"",
    "prettier:write": "npx prettier --write \"./*.js\"",
    "test:unit": "jest --config jest.config.js",
    "test": "npm run lint:fix && npm run prettier:write && npm run test:unit",
    "watch": "gulp watch",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yaohaixiao/types.js.git"
  },
  "keywords": [
    "JavaScript",
    "typeof",
    "detect",
    "types",
    "types.js"
  ],
  "author": "Robert Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yaohaixiao/types.js/issues"
  },
  "homepage": "https://github.com/yaohaixiao/types.js#readme",
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.14.8",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@yaohaixiao/outline.js": "^3.40.1",
    "babel-jest": "^29.5.0",
    "buffer": "^6.0.3",
    "commitlint": "^17.4.4",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-jest": "^3.0.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-connect": "^5.7.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-less": "^5.0.0",
    "gulp-livereload": "^4.0.2",
    "gulp-open": "^3.0.1",
    "gulp-pug": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-run": "^1.7.1",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-watch": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-html-reporter": "^3.7.0",
    "jsdom": "^22.1.0",
    "less-plugin-autoprefix": "^2.0.0",
    "lint-staged": "^13.1.2",
    "os": "^0.1.2",
    "prettier": "^2.8.4",
    "rollup": "^4.22.4",
    "typescript": "^5.5.3",
    "xmldom": "^0.6.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint"
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint:fix",
      "npm run prettier:write"
    ]
  }
}
